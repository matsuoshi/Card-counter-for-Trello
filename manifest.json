{
    "name": "Card counter for Trello",
    "manifest_version": 2,
    "author": "matsuoshi",
    "description": "Show card count in your Trello lanes",
    "version": "1.0.0",
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://trello.com/*"],
            "css": ["style.css"]
        }
    ]
}
